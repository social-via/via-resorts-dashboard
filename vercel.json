{
  "version": 2,
  "name": "via-resorts-dashboard",
  "buildCommand": "echo 'Static HTML - no build needed'",
  "outputDirectory": ".",
  "installCommand": "echo 'No dependencies to install'",
  "routes": [
    {
      "src": "/",
      "dest": "/dashboard-preview.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard-preview.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}
